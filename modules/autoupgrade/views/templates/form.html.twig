<div class="bootstrap" id="{{ name }}Block">
  <div class="panel">
    <div class="panel-heading">
      {{ tabName }}
    </div>
    <div class="form-wrapper">
      {% for field in fields %}
        {{ field|raw }}
      {% endfor %}
    </div>
    <div class="panel-footer">
      <button
        type="submit"
        class="btn btn-default pull-right"
        value="{{ 'Save'|trans({}) }}"
        name="customSubmitAutoUpgrade"
      >
        <i class="process-icon-save"></i>
        {{ 'Save'|trans({}) }}
      </button>
    </div>
  </div>
</div>
