<?xml version="1.0"?>
<AccessRequest xml:lang="en-US">
	<AccessLicenseNumber>{ac_hit}</AccessLicenseNumber>
	<UserId>{usr}</UserId>
	<Password>{pwd}</Password>
</AccessRequest>
<?xml version="1.0"?>
<RatingServiceSelectionRequest xml:lang="en-US">
	<Request>
		<TransactionReference>
			<CustomerContext>HIT TECH Rate Request</CustomerContext>
			<XpciVersion>1.0001</XpciVersion>
		</TransactionReference>
		<RequestAction>Rate</RequestAction>
		<RequestOption>shop</RequestOption>
	</Request>
	{customer_classification}
	<Shipment>
		<Shipper>
		<Name>{from_name}</Name>
		<ShipperNumber>{from_acc}</ShipperNumber>
			<Address>
				<City>{from_city}</City>
				<StateProvinceCode>{from_state}</StateProvinceCode>
				<CountryCode>{from_country}</CountryCode>
				<PostalCode>{from_postal}</PostalCode>
			</Address>
		</Shipper>
		<ShipTo>
			<Address>
				<City>{to_city}</City>
				<CountryCode>{to_country}</CountryCode>
				<PostalCode>{to_postal}</PostalCode>
			</Address>
		</ShipTo>
		{cod}
		<ShipFrom>
			<Address>
				<City>{from_city}</City>
				<StateProvinceCode>{from_state}</StateProvinceCode>
				<CountryCode>{from_country}</CountryCode>
				<PostalCode>{from_postal}</PostalCode>
			</Address>
		</ShipFrom>
		{package}
		<RateInformation>
			<NegotiatedRatesIndicator/>
		</RateInformation>
		<TaxInformationIndicator/>
	</Shipment>
</RatingServiceSelectionRequest>
