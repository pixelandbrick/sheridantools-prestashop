<root>
<FreightShipRequest>
      <Miscellaneous>
         <ReleaseID>07.12.2008</ReleaseID>
         <WSVersion>21.0.11</WSVersion>
      </Miscellaneous>
      <Shipment>
         <Commodity>
            <Description>{content}</Description>
           {dimensions}
            <FreightClass>110</FreightClass>
           {pcs}
         </Commodity>
         <DensityEligibleIndicator />
         <Documents>
            <Image>
               <Format>
                  <Code>01</Code>
               </Format>
               <LabelsPerPage>1</LabelsPerPage>
               <PrintFormat>
                  <Code>02</Code>
               </PrintFormat>
               <PrintSize>
                  <Length>8</Length>
                  <Width>11</Width>
               </PrintSize>
               <Type>
                  <Code>30</Code>
               </Type>
            </Image>
         </Documents>
         <HandlingUnitOne>
            <Quantity>2</Quantity>
            <Type>
               <Code>PLT</Code>
            </Type>
         </HandlingUnitOne>
         <PaymentInformation>
            <Payer>
               <AccountType>1</AccountType>
               {toaddress}
               <AttentionName>{payname}</AttentionName>
               <Name>{payname}</Name>
               <Phone>
                  <Number>{paynumber}</Number>
               </Phone>
               <ShipperNumber>{accountnumber}</ShipperNumber>
            </Payer>
            <ShipmentBillingOption>
               <Code>10</Code>
            </ShipmentBillingOption>
         </PaymentInformation>
         <PickupRequest>
            {pickup}
            <Requester>
               <AttentionName>{shipattention}</AttentionName>
               <EMailAddress>{shipmail}</EMailAddress>
               <Name>{shippercompanyname}</Name>
               <Phone>
                  <Number>{shipperphone}</Number>
               </Phone>
            </Requester>
         </PickupRequest>
         <Service>
            <Code>308</Code>
         </Service>
         <ShipFrom>
            {fromaddress}
            <AttentionName>{shipattention}</AttentionName>
            <Name>{shippercompanyname}</Name>
            <Phone>
               <Number>{shipperphone}</Number>
            </Phone>
         </ShipFrom>
         <ShipTo>
            {toaddress}
            <AttentionName>{payname}</AttentionName>
            <Name>{payname}</Name>
            <Phone>
               <Number>{paynumber}</Number>
            </Phone>
         </ShipTo>
         <ShipperNumber>{accountnumber}</ShipperNumber>
         <TimeInTransitIndicator />
      </Shipment>
   </FreightShipRequest>
</root>